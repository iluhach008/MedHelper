{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "is": {
            "projectType": "application",
            "schematics": {},
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/is",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.css",
                            "src/assets/css/bootstrap.min.css",
                            "src/assets/css/apexcharts.css",
                            "src/assets/css/tippy/tippy.css",
                            "src/assets/css/flatpickr.min.css",
                            "src/assets/css/select2.min.css",
                            "src/assets/css/quill/quill.snow.css",
                            "src/assets/css/quill/quill.core.css",
                            "src/assets/css/filepond.min.css",
                            "src/assets/css/filepond-plugin-image-preview.min.css",
                            "src/assets/css/swiper-bundle.min.css",
                            "src/assets/css/style.css"
                        ],
                        "scripts": [
                            "src/assets/js/gsap/gsap.min.js",
                            "src/assets/js/gsap/ScrollToPlugin.min.js",
                            "src/assets/js/gsap/ScrollTrigger.min.js",
                            "src/assets/js/vendor/popper.min.js",
                            "src/assets/js/vendor/jquery.min.js",
                            "src/assets/js/vendor/bootstrap.bundle.min.js",
                            "src/assets/js/vendor/imagesloaded.pkgd.min.js",
                            "src/assets/js/vendor/simplebar.min.js",
                            "src/assets/js/vendor/tippy-bundle.umd.min.js",
                            "src/assets/js/vendor/grid/masonry.pkgd.min.js",
                            "src/assets/js/vendor/grid/isotope.pkgd.min.js",
                            "src/assets/js/vendor/charts/circle-progress.min.js",
                            "src/assets/js/vendor/charts/echarts.common.min.js",
                            "src/assets/js/vendor/charts/apexcharts/apexcharts.min.js",
                            "src/assets/js/vendor/cleave/cleave.min.js",
                            "src/assets/js/vendor/cleave/addons/cleave-phone.us.js",
                            "src/assets/js/vendor/jqvmap/jquery.vmap.min.js",
                            "src/assets/js/vendor/jqvmap/jquery.vmap.world.js",
                            "src/assets/js/vendor/jqvmap/jquery.vmap.sampledata.js",
                            "src/assets/js/vendor/jquery.star-rating-svg.min.js",
                            "src/assets/js/vendor/calendar/flatpickr/flatpickr.min.js",
                            "src/assets/js/vendor/calendar/flatpickr/en.js",
                            "src/assets/js/vendor/select2.min.js",
                            "src/assets/js/vendor/editors/quill.min.js",
                            "src/assets/js/vendor/filepond/filepond-plugin-image-preview.min.js",
                            "src/assets/js/vendor/filepond/filepond.min.js",
                            "src/assets/js/vendor/swiper-bundle.min.js",
                            "src/assets/js/vendor/scrollmagic/ScrollMagic.min.js",
                            "src/assets/js/vendor/scrollmagic/debug.addIndicators.min.js",
                            "src/assets/js/components.js",
                            "src/assets/js/common.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "is:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "is:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "is:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.css"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "tsconfig.app.json",
                            "tsconfig.spec.json",
                            "e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "is:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "is:serve:production"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "is",
    "cli": {
        "analytics": "8aaabc6e-cdb6-464e-9475-b41db825ad16"
    }
}